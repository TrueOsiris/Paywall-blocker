{
  "name": "Paywall-blocker",
  "version": "0.1",
  "description": "Remove paywall from newsletters",
  "icons": {
	"16": "icons/icon_16.png",
	"48": "icons/icon_48.png",
	"128": "icons/icon_128.png"
  },
  "manifest_version": 2,
  "permissions": [
					"webRequest",
					"webRequestBlocking",
					"browsingData", 
					"tabs",
					"*://*.demorgen.be/",
					"*://*.tijd.be/",
					"*://*.telegraaf.nl/",
					"*://*.bloomberg.com/", "*://*.bwbx.io/*"],
 
  "background": {
    "scripts": ["background.js"],
	"persistent": true
  }
}